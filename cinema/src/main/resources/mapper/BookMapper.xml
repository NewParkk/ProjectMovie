<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.spring.cinema.mapper.BookMapper">

	<!-- 아이디로 예약조회 -->
	<select id="getBookByuserId" parameterType="string" resultType="Book">
				SELECT *
				FROM movie_booking
				 WHERE user_id = #{userId}
	</select>
	
</mapper>